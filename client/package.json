{
  "name": "expressedrealms.client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npx vite",
    "build": "npx run-p type-check \"build-only {@}\" --",
    "preview": "npx vite preview",
    "build-only": "npx vite build",
    "type-check": "npx vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "component-tests": "sudo chown -hR $(whoami) node_modules/; npx cypress run --component --browser chromium",
    "open-cypress": "sudo chown -hR $(whoami) node_modules/; npx cypress open",
    "vitest": "sudo chown -hR $(whoami) node_modules/; npx vitest --run",
    "vitest-pipeline": "npx vitest --coverage"
  },
  "dependencies": {
    "@he-tree/vue": "^2.9.4",
    "@primevue/themes": "^4.1.1",

    "eslint-plugin-vue": "^9.29.1",
    "jest-matchmedia-mock": "^1.1.0",
    "jsdom": "^26.0.0",
    "mocha-junit-reporter": "^2.2.1",
    "npm-run-all2": "^7.0.1",
    "sass": "^1.80.4",
    "ts-standard": "^12.0.2",
